  //     "blockClass": "layout-option"
  //   }
  // },
  // cantidad de productos
  "flex-layout.row#productCountMobile": {
    "children": ["total-products.v2"],
    "props": {
      "width":"25%",
      "blockClass": "productCountMobileRow"
    }
  },


  // filtro
  "flex-layout.col#filterMobile": {
    "children": ["filter-navigator.v3"],
    "props": {
      "width":"48%",
      "blockClass": "filterMobileCol"
    }
  }, 

  //FIN VERSION MOBILE

  // FORMATO PARA MOSTRAR PRODUCTOS
  "gallery": {
    "props": {
      "layouts": [
        {
          "name": "grid",
          "component": "GridSummary",
          "itemsPerRow": {
            "(min-width:1600px)": 4,
            "(min-width:1367px)": 4,
            "(min-width:1025px)": 4,
            "desktop": 4,
            "tablet": 3,
            "phone": 1
          }
        },
        {
          "name": "list",
          "component": "ListSummary",
          "itemsPerRow": 1
        }
      ],
      "ListSummary": "product-summary.shelf",
      "GridSummary": "product-summary.shelf"
    }
  },

  //FILTRO NAVIGATOR
  "filter-navigator.v3": {
    "blocks":["sidebar-close-button"],
    "props": {
      "closeOnOutsideClick": true,
      "initiallyCollapsed": true,
      "navigationTypeOnMobile": "collapsible"
    }
  },


  // BUSQUEDA VACIA
  "search-not-found-layout": {
    "children": [
      "flex-layout.row#searchbread-desktop",
      "flex-layout.row#notfound"
    ]
  },
  
  "flex-layout.row#notfound": {
    "props":{
      "marginTop":7,
      "marginBottom":9,
      "horizontalAlign":"center",
      "preventHorizontalStretch":true,
      "blockClass":"not-found"
    },
    "children": [
      "flex-layout.col#notfound"
    ]
  },
  "flex-layout.col#notfound":{
    "props":{
      /* "paddingLeft":10, */
      /* "paddingRight":5, */
      "horizontalAlign":"center",
      "blockClass":"build-container-search"
    },
    "children":["not-found"]
  }

 
}
  

 