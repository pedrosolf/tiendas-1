  "rich-text#preguntas-frecuentes-seccion-6-19-pregunta": {
    "props": {
      "text": "#### ¿LOS PRODUCTOS SON APTOS PARA LAVAVAJILLA?",
      "blockClass": "preguntas-frecuentes-pregunta"
    }
  },
  "disclosure-content#preguntas-frecuentes-seccion-6-19": {
    "children": ["rich-text#preguntas-frecuentes-seccion-6-19-respuesta"]
