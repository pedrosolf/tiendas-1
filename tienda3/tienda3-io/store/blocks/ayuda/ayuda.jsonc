  "rich-text#preguntas-frecuentes-seccion-6-11-pregunta": {
    "props": {
      "text": "#### ¿QUÉ ES LA AISLACIÓN POR VACÍO?",
      "blockClass": "preguntas-frecuentes-pregunta"
    }
  },
  "disclosure-content#preguntas-frecuentes-seccion-6-11": {
    "children": ["rich-text#preguntas-frecuentes-seccion-6-11-respuesta"]
  },
  "rich-text#preguntas-frecuentes-seccion-6-11-respuesta": {
    "props": {
      "text": "La aislación por vacío es el proceso mediante el cual se remueve el aire entre la pared exterior e interior del termo. De esta forma se evita la transferencia de energía térmica por convección.",
      "blockClass": "preguntas-frecuentes-respuesta"
    }
  },
  "disclosure-layout#preguntas-frecuentes-seccion-6-12": {
