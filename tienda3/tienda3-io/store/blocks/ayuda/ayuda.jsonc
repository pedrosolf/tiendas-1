  "rich-text#preguntas-frecuentes-seccion-6-16-pregunta": {
    "props": {
      "text": "#### ¿QUÉ CEPILLO DEBO USAR PARA LIMPIAR EL INTERIOR DE ACERO INOXIDABLE?",
      "blockClass": "preguntas-frecuentes-pregunta"
    }
  },
  "disclosure-content#preguntas-frecuentes-seccion-6-16": {
