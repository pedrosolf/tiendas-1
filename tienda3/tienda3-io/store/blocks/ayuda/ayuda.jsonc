  "rich-text#preguntas-frecuentes-seccion-6-10-pregunta": {
    "props": {
      "text": "#### ¿LOS PRODUCTOS STANLEY SON LIBRES DE BPA?",
      "blockClass": "preguntas-frecuentes-pregunta"
