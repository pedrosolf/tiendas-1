  "rich-text#preguntas-frecuentes-seccion-6-19-respuesta": {
    "props": {
      "text": "En la base del termo se indica si es apto para lavavajillas. En caso de que así sea, recomendamos ponerlos en la bandeja superior. Por favor lave a mano los productos que no sean aptos para hacerlo.",
      "blockClass": "preguntas-frecuentes-respuesta"
    }
  },
